{
  "items": "minecraft:gunpowder",
  "complement": true,
  "events": {
    "event": "any",
    "commands": {
      "type": "commander:defaulted",
      "then": {
        "type": "commander:commands",
        "selector": "minecraft:direct_killer_entity",
        "commands": [
          "/cmd:explode ~ ~ ~ 1"
        ]
      },
      "commands": {
        "type": "commander:commands",
        "condition": {
          "condition": "commander:expression",
          "value": "hasContext(\"killer_entity\")"
        },
        "selector": "minecraft:direct_killer_entity",
        "commands": [
          "/cmd:explode ${{ killer_entity.getUUID }} ~ ~ ~ 1"
        ]
      }
    }
  }
}