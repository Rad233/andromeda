<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <!-- Must be a full match -->
        <Class name="~.*\.mixin\..*Mixin" />
        <Bug pattern="BC_IMPOSSIBLE_CAST, MS_SHOULD_BE_FINAL" />
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2, PA_PUBLIC_PRIMITIVE_ATTRIBUTE, BC_UNCONFIRMED_CAST" />
    </Match>

    <Match>
        <Class name="me.melontini.andromeda.modules.mechanics.throwable_items.data.ParticleCommand" />
        <Method name="sendParticlePacket" />
        <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
    </Match>
    <Match>
        <Class name="me.melontini.andromeda.base.ModuleManager" />
        <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
    </Match>
</FindBugsFilter>